{
  "name": "@fett/utils",
  "version": "0.0.9",
  "description": "js utils",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "author": "chrissong1994 <18557511748@163.com>",
  "license": "MIT",
  "type": "module",
  "bugs": {
    "url": "https://github.com/ChrisSong1994/utils/issues"
  },
  "homepage": "https://github.com/ChrisSong1994/utils",
  "scripts": {
    "dev": "rollup  -c -w  --environment NODE_ENV:development",
    "build": "npm run clean && rollup  -c  --environment NODE_ENV:production",
    "example:esm": "cd examples/esm && npm run dev",
    "example:umd": "npm run build  && serve . -p 3003",
    "clean": "rimraf dist",
    "lint": "eslint src --ext .ts",
    "commit": "cz",
    "prettier": "prettier --write 'src/**/*.ts'",
    "format": "npm run prettier && npm run lint",
    "prepublish": "npm run build",
    "publish": "npm publish --access public",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "prepare": "husky && husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ChrisSong1994/utils.git"
  },
  "keywords": [
    "npm",
    "package",
    "template",
    "rollup"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "node  scripts/verify-commit-msg.js"
    }
  },
  "lint-staged": {
    "src/**/*.ts?(x)": [
      "npm run format"
    ],
    "*.lint": "prettier --write"
  },
  "dependencies": {
    "dayjs": "^1.11.13"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.1",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "@vitest/coverage-v8": "3.0.7",
    "chalk": "^5.4.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "rollup": "^4.19.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-esbuild": "^6.1.1",
    "serve": "^14.2.3",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.5.3",
    "vitest": "^3.1.2"
  },
  "packageManager": "pnpm@9.15.0+sha512.76e2379760a4328ec4415815bcd6628dee727af3779aaa4c914e3944156c4299921a89f976381ee107d41f12cfa4b66681ca9c718f0668fa0831ed4c6d8ba56c"
}
